{
  "expo": {
    "name": "Luuma UCAD",
    "slug": "luuma-ucad",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "luumaucadclient",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "sn.luumaucad.client"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "sn.luumaucad.client",
      "googleServicesFile": "./keys/google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-asset",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/splash-icon.png"
        }
      ],
      "expo-secure-store",
      "expo-web-browser",
      "expo-notifications"
    ],
    "extra": {
      "eas": {
        "projectId": "13f81676-34fa-488a-a230-3a4d39f5e41f"
      }
    },
    "owner": "luuma_ucad"
  }
}
